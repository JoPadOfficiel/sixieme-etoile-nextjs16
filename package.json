{
	"name": "supastarter-nextjs",
	"private": true,
	"scripts": {
    "dev": "dotenv -c -- turbo dev --concurrency 15",
    "build": "dotenv -c -- turbo build",
    "lint": "biome lint .",
    "clean": "turbo clean",
    "format": "biome format . --write",
    "e2e": "pnpm --filter web e2e",
    "seed:vtc": "pnpm --filter @repo/database db:seed:vtc"
  },
	"engines": {
		"node": ">=20"
	},
	"packageManager": "pnpm@9.3.0",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@repo/tsconfig": "workspace:*",
		"@types/node": "^22.9.0",
		"dotenv-cli": "^7.4.4",
		"turbo": "^2.3.3",
		"typescript": "5.6.3"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "19.2.7",
			"@types/react-dom": "19.2.3",
			"next": "16.0.2-canary.33"
		}
	}
}
